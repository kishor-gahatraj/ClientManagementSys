                        <a href="#">
                            <i class="fa fa-calendar icon"></i>
                            <span>AMC</span>
                            <i class="bx bxs-chevron-right"></i>
                        </a>
                        <ul class="sub-menu">
                            <li>
                                @Html.ActionLink("Add AMC", "Create", "AMCs")
                            </li>
                            <li>
                                @Html.ActionLink("AMC List", "Index", "AMCs")

                            </li>
                        </ul>
                @*    <li class="nav-item">
                        <a href="#">
                            <i class="fa fa-th-list" aria-hidden="true"></i>

                            <span>ProOrg List</span>
                        </a>
                        <i class="bx bxs-chevron-right"></i>
                    </li>*@
                    </div>
                    </div>
                    <div class="dashboard">
                        <div class="top-nav">
                            <p class="cms">Client Management System</p>
                            <a href="#">Logout</a>
                        </div>
                        <div class="main">
                            <div class="client">
                                <div class="add-client">
                                    <p> Edit AMC</p>
                                </div>
                            </div>
                            <div class="containers">
                                <div class="Contact" id="add-client">
                                    <div class="Contact" id="add-client">
                                        <form asp-action="Edit">
                                            <div asp-validation-summary="ModelOnly" class="text-danger"></div>
                                            <input type="hidden" asp-for="AMC_Id" />
                                            @*<div>
                                                <label asp-for="AMC_Name" class="control-label">AMC Name</label>
                                                <input type="text" asp-for="AMC_Name" class="form-control" />
                                                <span asp-validation-for="AMC_Name" class="text-danger"></span>
                                            </div>*@
                                            <div>
                                                <label asp-for="Maintenance_Charge" class="control-label"> Maintenance Charge</label>
                                                <input type="text" asp-for="Maintenance_Charge" class="form-control" />
                                                <span asp-validation-for="Maintenance_Charge" class="text-danger"></span>
                                            </div>
                                            <div>
                                                <label asp-for="Maintenance_Date" class="control-label">Maintenance Date</label>
                                                <br />
                                                <input asp-for="Maintenance_Date" type="date" class="form-control" />
                                                <span asp-validation-for="Maintenance_Date" class="text-danger"></span>
                                            </div>
                                            <div>
                                                <label asp-for="Product_Id" class="control-label">Product Name</label><br />

                                                <select asp-for="Product_Id" class="form-control" asp-items="ViewBag.Product_Id"></select>
                                            </div>
                                            <div>
                                                <label asp-for="Org_Id" class="control-label">Organization Name</label>
                                                <br />
                                                <select asp-for="Org_Id" class="form-control" asp-items="ViewBag.Org_Id"></select>
                                            </div>

                                          @*  <div>
                                            </div>*@
                                            <div>
                                               
                                                <br/>
                                                <input type="submit" value="Edit" class="registerbtn"></input>
                                               
                                            </div>
                                            <div class ="space">

                                                </div>
                                        </form>
                                    </div>
                                </div>
                            </div>
                        </div>
                        <div class="footer">
                            <h6>Client Management system 2022 by Silicon soft Nepal</h6>
                        </div>
                    </div>
            </div>
            <script src="/js/navbar.js"></script>
</body>
</html>




























